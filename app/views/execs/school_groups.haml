/ <pre>
/ #{@results.to_yaml}
/ </pre>
.container
  %h1 School List
  %table.table.striped.table-bordered.table-hover
    %tr
      %th School Name
      %th Applicant Count
    - @results.each do |result|
      %tr
        %td= link_to(result.name, "execs/school/" + result.schools_id.to_s, target: "blank")
        %td= result.applicant_count